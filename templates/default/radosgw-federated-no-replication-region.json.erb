{
  "name": "<%= @region %>-<%= @zone %>",
  "api_name": "<%= @region %>-<%= @zone %>",
  "is_master": "true",
  "endpoints": ["http:\/\/<%= @zone_url %>:<%= @zone_port %>\/"],
  "hostnames": [
      <% @s3hostnames.each_with_index do |h, index| -%>
          "<%= h %>"<%= ',' unless index == @s3hostnames.size - 1 %>
      <% end -%>
  ],
  "hostnames_s3website": [
      <% @s3hostnames_website.each_with_index do |h, index| -%>
          "<%= h %>"<%= ',' unless index == @s3hostnames.size - 1 %>
      <% end -%>
  ],
  "master_zone": "<%= @region %>-<%= @zone %>",
  "zones": [
        {
          "name": "<%= @region %>-<%= @zone %>",
          "endpoints": [
                "http:\/\/<%= @zone_url %>:<%= @zone_port %>\/"],
          "log_meta": "true",
          "log_data": "true"
        }
  ],
  "placement_targets": [
   {
     "name": "default-placement",
     "tags": []
   }
  ],
  "default_placement": "default-placement"
}
