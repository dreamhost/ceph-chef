{
  "domain_root": ".<%=@zonegroup%>-<%=@zone%>.rgw",
  "control_pool": ".<%=@zonegroup%>-<%=@zone%>.rgw.control",
  "gc_pool": ".<%=@zonegroup%>-<%=@zone%>.rgw.gc",
  "log_pool": ".<%=@zonegroup%>-<%=@zone%>.log",
  "intent_log_pool": ".<%=@zonegroup%>-<%=@zone%>.intent-log",
  "usage_log_pool": ".<%=@zonegroup%>-<%=@zone%>.usage",
  "user_keys_pool": ".<%=@zonegroup%>-<%=@zone%>.users",
  "user_email_pool": ".<%=@zonegroup%>-<%=@zone%>.users.email",
  "user_swift_pool": ".<%=@zonegroup%>-<%=@zone%>.users.swift",
  "user_uid_pool": ".<%=@zonegroup%>-<%=@zone%>.users.uid",
  "metadata_heap": ".<%=@zonegroup%>-<%=@zone%>.rgw.meta",
  "system_key": {
    "access_key": "<%=@access_key%>",
    "secret_key": "<%=@secret_key%>"
  },
  "placement_pools": [
    {
      "key": "default-placement",
      "val": {
        "index_pool": ".<%=@zonegroup%>-<%=@zone%>.rgw.buckets.index",
        "data_pool": ".<%=@zonegroup%>-<%=@zone%>.rgw.buckets",
        "data_extra_pool": ".<%=@zonegroup%>-<%=@zone%>.rgw.buckets.extra"
      }
    }
